{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/signup",
      "dest": "/api/signup.js"
    },
    {
      "src": "/api/reset",
      "dest": "/api/reset.js"
    },
    {
      "src": "/api/depositmonitor",
      "dest": "/api/depositmonitor.js"
    },
    {
      "src": "/",
      "dest": "/login.html"
    }
}
